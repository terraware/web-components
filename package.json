{
  "name": "@terraware/web-components",
  "version": "2.0.56",
  "author": "Terraformation Inc.",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/terraware/web-components.git"
  },
  "bugs": {
    "url": "https://github.com/terraware/web-components/issues"
  },
  "homepage": "https://github.com/terraware/web-components#readme",
  "private": false,
  "main": "dist/index.js",
  "dependencies": {
    "@date-io/date-fns": "^2.14.0",
    "@date-io/moment": "^2.16.1",
    "@dnd-kit/core": "^6.0.5",
    "@dnd-kit/sortable": "^7.0.1",
    "@emotion/react": "^11.9.3",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.8.7",
    "@mui/styles": "^5.8.7",
    "@mui/x-date-pickers": "^5.0.12",
    "classnames": "^2.3.1",
    "date-fns": "^2.28.0",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.37",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-responsive": "^9.0.0-beta.6",
    "react-sortable-hoc": "^2.0.0",
    "sass": "^1.35.1",
    "tinycolor2": "^1.4.2",
    "web-vitals": "^3.0.0"
  },
  "scripts": {
    "start": "start-storybook",
    "build": "build-storybook -c .storybook -o public",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook",
    "build-storybook": "build-storybook -s public",
    "build-dictionary": "./style-dictionary/generate.sh && ./style-dictionary2/generate.sh",
    "transpile": "rm -rf dist && NODE_ENV=production babel src/ --extensions '.tsx,.ts' --out-dir dist --copy-files --no-copy-ignored && cp package.json dist/",
    "generate-assets": "yarn svgr --icon --title-prop --no-dimensions --typescript -d src/components/svg assets",
    "generate-types": "yarn -p typescript tsc",
    "build-dist": "yarn transpile && yarn generate-types",
    "lint": "tslint --project tsconfig.json",
    "license-report": "license-report --only=prod --department.value=terraware --relatedTo.value=web-components --output=html > license-report.html",
    "license-report-dist": "license-report --only=prod --department.value=terraware --relatedTo.value=web-components --output=html > dist/license-report.html",
    "check-updates": "yarn build-dictionary && yarn storybook"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.10",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@jsdevtools/version-bump-prompt": "^6.1.0",
    "@storybook/addon-actions": "^6.3.7",
    "@storybook/addon-essentials": "^6.3.7",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/node-logger": "^6.3.7",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.3.7",
    "@svgr/cli": "^6.5.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^14.0.0",
    "fs": "^0.0.1-security",
    "hex-rgb": "^5.0.0",
    "license-report": "^6.0.0",
    "react-scripts": "^4.0.3",
    "style-dictionary": "^3.0.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^5.0.0",
    "tslint-react-hooks": "^2.2.2",
    "typescript": "^4.3.2"
  },
  "resolutions": {
    "babel-loader": "8.3.0",
    "@storybook/react/**/loader-utils": "^2.0.4"
  }
}
