{
  "name": "@terraware/web-components",
  "version": "0.0.3",
  "author": "Terraformation Inc.",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/terraware/web-components.git"
  },
  "bugs": {
    "url": "https://github.com/terraware/web-components/issues"
  },
  "homepage": "https://github.com/terraware/web-components#readme",
  "private": false,
  "files": [
    "dist"
  ],
  "main": "index.js",
  "dependencies": {
    "@date-io/moment": "^2.10.11",
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.58",
    "@material-ui/pickers": "^3.3.10",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "classnames": "^2.3.1",
    "license-report": "^6.0.0",
    "moment": "^2.29.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^4.0.3",
    "react-sortable-hoc": "^2.0.0",
    "sass": "^1.35.1",
    "tinycolor2": "^1.4.2",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "start-storybook",
    "build": "build-storybook -c .storybook -o public",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "echo '>>>>> Starting Story-Book! <<<<<' && start-storybook",
    "build-storybook": "echo '>>>>> Building Story-Book! <<<<<' && build-storybook -s public",
    "build-dictionary": "echo '>>>> Building dictionary of styles! <<<<<' && ./style-dictionary/generate.sh",
    "build-dist": "rm -rf dist && NODE_ENV=production babel src/ --out-dir dist --copy-files --no-copy-ignored && cp package.json dist/ && rm -rf App.css fonts stories logo.svg react-app-env.d.ts theme.ts",
    "lint": "tslint --project tsconfig.json",
    "license-report": "license-report --only=prod --department.value=terraware --related.value=terraware --output=html > dist/license-report.html",
    "check-updates": "yarn build-dictionary && yarn storybook"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "@storybook/addon-actions": "^6.3.7",
    "@storybook/addon-essentials": "^6.3.7",
    "@storybook/addon-links": "^6.3.7",
    "@storybook/node-logger": "^6.3.7",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.3.7",
    "babel-cli": "^6.26.0",
    "babel-preset-react-app": "^10.0.1",
    "style-dictionary": "^3.0.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^5.0.0",
    "tslint-react-hooks": "^2.2.2",
    "typescript": "^4.3.2",
    "webpack": "^4.44.2"
  },
  "resolutions": {
    "babel-loader": "8.1.0"
  }
}
